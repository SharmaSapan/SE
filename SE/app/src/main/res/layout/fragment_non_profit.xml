<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey"
    tools:context="com.">
    <!--android:theme="@style/Theme.4P02App.NoActionBar"-->


    <ImageButton
        android:id="@+id/btnAddtoFavs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:background="@color/fui_transparent"
        android:contentDescription="TODO"
        android:src="@drawable/ic_star"
        android:tint="@color/teal_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/profilePart" />

    <ImageButton
        android:id="@+id/btnRemFromFavs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:background="@color/fui_transparent"
        android:contentDescription="TODO"
        android:src="@drawable/ic_baseline_star_24"
        android:tint="@color/teal_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/profilePart" />
    
    <ImageView
        android:id="@+id/profilePic"
        android:layout_width="177dp"
        android:layout_height="137dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="97dp"
        android:background="@color/teal_darkgrey"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/profilePart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameDisplay"
        app:layout_constraintVertical_bias="0.29" />

    <TextView
        android:id="@+id/profilePart"
        android:layout_width="390dp"
        android:layout_height="212dp"
        android:layout_margin="10dp"
        android:background="@color/teal_500"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.851" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="67dp"
        android:layout_height="31dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="68dp"
        android:text="ABOUT:"
        android:textAlignment="center"
        android:textColor="@color/teal_300"
        app:layout_constraintStart_toStartOf="@+id/profilePart"
        app:layout_constraintTop_toTopOf="@+id/profilePart" />

    <ImageView
        android:id="@+id/email"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="38dp"
        android:background="@color/teal_700"
        android:contentDescription="@string/todo"
        android:src="@drawable/ic_mail"
        app:layout_constraintBottom_toTopOf="@+id/profilePart"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintHorizontal_bias="0.288"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profilePic"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/map"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="38dp"
        android:background="@color/teal_700"
        android:contentDescription="TODO"
        android:src="@drawable/ic_location"
        app:layout_constraintBottom_toTopOf="@+id/profilePart"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profilePic"
        app:tint="@color/white" />


    <ImageView
        android:id="@+id/callOrg"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="38dp"
        android:background="@color/teal_700"
        android:contentDescription="@string/todo"
        android:src="@drawable/ic_phone"
        app:layout_constraintBottom_toTopOf="@+id/profilePart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.628"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profilePic"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/visitWebsite"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="38dp"
        android:background="@color/teal_700"
        android:contentDescription="@string/todo"
        android:src="@drawable/ic_website2"
        app:layout_constraintBottom_toTopOf="@+id/profilePart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.709"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/profilePic"
        app:tint="@color/white" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5"/>

    <TextView
        android:id="@+id/nameDisplay"
        android:layout_width="406dp"
        android:layout_height="85dp"
        android:layout_marginTop="36dp"
        android:textAlignment="center"
        android:textSize="30dp"
        android:fontFamily="@font/roboto_bold"
        android:textColor="@color/teal_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/npoAddress"
        android:layout_width="172dp"
        android:layout_height="46dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:textAlignment="center"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/profilePart"
        app:layout_constraintTop_toTopOf="@+id/profilePart" />

    <TextView
        android:id="@+id/profileContact"
        android:layout_width="172dp"
        android:layout_height="47dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="4dp"
        android:textAlignment="center"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/profilePart"
        app:layout_constraintTop_toTopOf="@+id/profilePart" />

    <TextView
        android:id="@+id/profileDesc"
        android:layout_width="367dp"
        android:layout_height="116dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="4dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/profilePart"
        app:layout_constraintEnd_toEndOf="@+id/profilePart"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/profilePart" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="67dp"
        android:layout_height="31dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="1dp"
        android:text="ADDRESS:"
        android:textAlignment="center"
        android:textColor="@color/teal_300"
        app:layout_constraintStart_toStartOf="@+id/profilePart"
        app:layout_constraintTop_toTopOf="@+id/profilePart" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="67dp"
        android:layout_height="31dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="57dp"
        android:text="CONTACT:"
        android:textAlignment="center"
        android:textColor="@color/teal_300"
        app:layout_constraintEnd_toEndOf="@+id/profilePart"
        app:layout_constraintTop_toTopOf="@+id/profilePart" />

</androidx.constraintlayout.widget.ConstraintLayout>
